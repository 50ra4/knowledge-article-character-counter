{
  "name": "knowledge-article-character-counter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:clear": "rimraf dist",
    "build:esm": "tsc -p ./tsconfig.build-esm.json",
    "build:lib": "tsc -p ./tsconfig.build.json",
    "build": "npm run build:clear && npm run build:esm && npm run build:lib",
    "check-type": "tsc --noEmit",
    "test": "jest -c ./jest.config.js",
    "start": "npm run count-article-character",
    "count-article-character": "ts-node --project tsconfig.scripts.json scripts/countCharacters.ts",
    "lint:prettier": "prettier --write '{src,test,tools}/**/*.{ts,tsx}'",
    "lint:eslint": "eslint --fix '{src,test,tools}/**/*.{ts,tsx}'",
    "lint": "npm run lint:prettier && npm run lint:eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,json}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/50ra4/ts-library-template.git"
  },
  "author": "50ra4",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/50ra4/ts-library-template/issues"
  },
  "homepage": "https://github.com/50ra4/ts-library-template#readme",
  "dependencies": {
    "date-fns": "2.28.0",
    "fp-ts": "2.12.1",
    "lodash.partial": "4.2.1"
  },
  "devDependencies": {
    "@types/jest": "28.1.3",
    "@types/lodash.partial": "4.2.7",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "commander": "9.3.0",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.1.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-environment-jsdom-global": "3.1.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "puppeteer": "15.1.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  }
}
